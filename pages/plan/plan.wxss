/* pages/plan/plan.wxss */
page{
  background-color: #f7f7f7;
  padding-bottom: 50rpx;
}
.page_box{
  width: 94%;
  margin: 0 auto;
}
.page_item{
  width: 100%;
  background: #fff;
  margin-top: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 6rpx;
  margin-bottom: 40rpx;
}


/*添加按钮*/
.planAdd{
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  bottom: 50rpx;
  left: 50%;
  margin-left: -40rpx;
}
.planAdd  image{
  width: 100%;
  height: 100%;
}

.page_list{
  font-size: 32rpx;
  color: #000;
}
.page_list view{
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.page_list image{
  width: 40rpx;
  height: 40rpx;
}